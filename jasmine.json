{
  "spec_dir": "dist/spec",
  "spec_files": [
    "**/*.spec.js"
  ],
  "helpers": [
    "helpers/**/*.js"
  ],
  "reporters": [
    {
      "name": "jasmine-spec-reporter#SpecReporter",
      "options": {
        "displayStacktrace": "pretty",
        "displaySuccessful": true
      }
    }
  ],
  "stopSpecOnExpectationFailure": true,
  "random": false
}
